<Algorithm>
  <Name>Algorithm 1</Name>
  <Command>
    <Name>Top Layer</Name>
    <Parameter>
      <Name>Lipid.Height</Name>
      <MinValue>120</MinValue>
      <MaxValue>200</MaxValue>
      <Step>5</Step>
      <Value>150</Value>
    </Parameter>
    <MeritComponent>
      <Type>AvDev</Type>
      <Polarization>S</Polarization>
      <MinWavelength>650</MinWavelength>
      <MaxWavelength>1080</MaxWavelength>
      <Weight>1</Weight>
    </MeritComponent>
    <Options>
      <SearchMethod>OneDimension</SearchMethod>
      <Display>Off</Display>
      <WavelengthIndexStep>1</WavelengthIndexStep>
      <MaxIter>1000</MaxIter>
      <TolFun>0</TolFun>
      <TolX>0.01</TolX>
      <MaxTime>1</MaxTime>
    </Options>
  </Command>
  <Command>
    <Name>Two Lower Layers</Name>
    <Parameter>
      <Name>Aqueous.Height</Name>
      <MinValue>680</MinValue>
      <MaxValue>1200</MaxValue>
      <Step>10</Step>
      <Value>850</Value>
      <OptStep>50</OptStep>
    </Parameter>
    <Parameter>
      <Name>Mucus.Roughness</Name>
      <MinValue>600</MinValue>
      <MaxValue>2600</MaxValue>
      <Step>50</Step>
      <Value>2500</Value>
      <OptStep>150</OptStep>
    </Parameter>
    <MeritComponent>
      <Type>StDev</Type>
      <Polarization>S</Polarization>
      <MinWavelength>750</MinWavelength>
      <MaxWavelength>800</MaxWavelength>
      <Weight>220</Weight>
    </MeritComponent>
    <MeritComponent>
      <Type>StDev</Type>
      <Polarization>S</Polarization>
      <MinWavelength>800</MinWavelength>
      <MaxWavelength>1080</MaxWavelength>
      <Weight>80</Weight>
    </MeritComponent>
    <Options>
      <SearchMethod>SimplexEx</SearchMethod>
      <Display>Off</Display>
      <WavelengthIndexStep>1</WavelengthIndexStep>
      <MaxIter>1000</MaxIter>
      <TolFun>0</TolFun>
      <TolX>0.005</TolX>
      <MaxTime>0.01</MaxTime>
    </Options>
  </Command>
</Algorithm>
